#
# Ubuntu 16.04 64bit in VBox 5
#

assert_spec 64bit

#set flavor   "machine"
set flavor   "gentoo"
set vm_ram   "5000M"

set use_vbox4 0
set use_vbox5 1

set use_rumpfs 1
# Write overlay only into ram
set use_ram_fs 0
# However read initial overlay from disk
set use_overlay_from_disk 0

set use_usb 1
set use_ps2 [have_spec ps2]

set use_vms 1
set use_cpu_load 0

source ${genode_dir}/repos/ports/run/vbox_win.inc
